import random
offices=[[],[],[]]
teachernames=["a","b","c","d","e","f","g","h"]
for name in teachernames:
    index=random.randint(0,2)
    offices[index].append(name)
i=1
for office in offices:
    print("office%d 的人数为：%d"%(i,len(office)))
    i+=1
    for name in office:
        print("%s"%name,end="\t")
    print("\n")
    print("-"*20)
